<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="se2203b.assignments.ifinance.NonAdminUserController">
    <children>
        <Label layoutX="129.0" layoutY="58.0" text="Creare a New User Account">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Label>
        <Label layoutX="110.0" layoutY="132.0" text="Full Name "/>
        <Label layoutX="128.0" layoutY="178.0" text="Adress"/>
        <Label layoutX="131.0" layoutY="205.0" text="Email"/>
        <Label layoutX="111.0" layoutY="251.0" text="Username"/>
        <Label layoutX="113.0" layoutY="277.0" text="Password"/>
        <Label layoutX="65.0" layoutY="303.0" text="Confirm Password"/>
        <TextField fx:id="fullName" layoutX="172.0" layoutY="128.0" prefHeight="25.0" prefWidth="228.0"/>
        <TextField fx:id="address" layoutX="171.0" layoutY="174.0" prefHeight="25.0" prefWidth="228.0"/>
        <TextField fx:id="email" layoutX="171.0" layoutY="201.0" prefHeight="25.0" prefWidth="228.0"/>
        <TextField fx:id="userName" layoutX="172.0" layoutY="247.0" prefHeight="25.0" prefWidth="228.0"/>
        <TextField fx:id="password" layoutX="172.0" layoutY="273.0" onAction="#passwordText" onKeyTyped="#passwordText"
                   prefHeight="25.0" prefWidth="228.0"/>
        <TextField fx:id="confirmPassword" layoutX="172.0" layoutY="299.0" onAction="#confirmPasswordText"
                   onKeyTyped="#confirmPasswordText" prefHeight="25.0" prefWidth="228.0"/>
        <Button fx:id="cancelBtn" layoutX="244.0" layoutY="361.0" mnemonicParsing="false" onAction="#cancel"
                prefHeight="25.0" prefWidth="57.0" text="Cancel"/>
        <Button fx:id="saveBtn" layoutX="349.0" layoutY="361.0" mnemonicParsing="false" onAction="#save"
                prefHeight="25.0" prefWidth="57.0" text="Save"/>
    </children>
</AnchorPane>

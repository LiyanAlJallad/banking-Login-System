<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pane" prefHeight="250.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="se2203b.assignments.ifinance.UserAccountController">
    <children>
        <Label layoutX="12.0" layoutY="42.0" text="Username: "/>
        <Label layoutX="14.0" layoutY="96.0" onKeyTyped="#passwordText" text="Password: "/>
        <TextField fx:id="password" layoutX="80.0" layoutY="92.0" onAction="#passwordText" onKeyTyped="#passwordText"
                   prefHeight="25.0" prefWidth="206.0"/>
        <TextField fx:id="userName" layoutX="80.0" layoutY="38.0" prefHeight="25.0" prefWidth="206.0"/>
        <Button fx:id="cancelBtn" layoutX="131.0" layoutY="200.0" mnemonicParsing="false" onAction="#cancel"
                prefHeight="25.0" prefWidth="56.0" text="Cancel"/>
        <Button fx:id="OKBtn" layoutX="220.0" layoutY="200.0" mnemonicParsing="false" onAction="#OK" prefHeight="25.0"
                prefWidth="56.0" text="OK"/>
    </children>
</AnchorPane>
